<%= form_for(@diagnosis) do |f| %>
  <% if @diagnosis.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@diagnosis.errors.count, "error") %> prohibited this diagnosis from being saved:</h2>

      <ul>
      <% @diagnosis.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :protocol %><br>
    <%= f.text_field :protocol %>
  </div>
  <div class="field">
    <%= f.label :version %><br>
    <%= f.number_field :version, in: 1..100 %>
  </div>
  <div class="field">
    <%= f.label :authorized_key %><br>
    <%= f.text_field :authorized_key %>
  </div>
  <div class="field">
    <%= f.label :equipment %><br>
    <%= f.text_field :equipment %>
  </div>
  <div class="field">
    <%= f.label :year %><br>
    <%= f.number_field :year, in: 2015..2029 %>
  </div>
  <div class="field">
    <%= f.label :month %><br>
    <%= f.number_field :month, in: 1..12 %>
  </div>
  <div class="field">
    <%= f.label :day %><br>
    <%= f.number_field :year, in: 1..31 %>
  </div>
  <div class="field">
    <%= f.label :hour %><br>
    <%= f.number_field :year, in: 0..23 %>
  </div>
  <div class="field">
    <%= f.label :minute %><br>
    <%= f.number_field :year, in: 0..59 %>
  </div>
  <div class="field">
    <%= f.label :second %><br>
    <%= f.number_field :year, in: 0..60 %>
  </div>
  <div class="field">
    <%= f.label :time_zone %><br>
    <%= f.text_field :year %>
