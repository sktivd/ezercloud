<%= simple_form_for(@notification) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :follow %>
    <%= f.input :tag %>
    <%= f.input :url %>
    <%= f.input :data %>
    <%= f.input :mailer%>
    <%= f.input :every %>
    <%= f.input :account_id %>
    <%= f.input :redirect_path%>
    <%= f.input :expired_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
