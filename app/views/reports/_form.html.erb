<%= simple_form_for(@report) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :equipment, collection: all_equipment.map{ |e| e.equipment } %>
    <%= f.input :serial_number %>
    <%= f.input :date %>
    <%= f.input :assay_kit%>
    <%= f.input :reagent %>
    <%= f.input :document, as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
