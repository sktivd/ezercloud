<%= simple_form_for(@report) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :equipment %>
    <%= f.input :serial_number %>
    <%= f.input :date %>
    <%= f.association :reagent %>
    <%= f.input :document, as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
