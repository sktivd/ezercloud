<% content_for :header do %>
  <%= javascript_include_tag "https://maps.google.com/maps/api/js?v=3&key=AIzaSyDk8p2C8Mns_FP3hdAEb4sdK9ZAtqKk0cA&libraries=geometry,places&language=&hl=&region=", async: Rails.env.production?, defer: Rails.env.production?, 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'pie_markerclusterer/markerclusterer.js', 'data-turbolinks-track' => true %>
  <% if false %>
  <script async defer src="https://maps.google.com/maps/api/js?v=3&key=AIzaSyDk8p2C8Mns_FP3hdAEb4sdK9ZAtqKk0cA&libraries=geometry,places&language=&hl=&region="></script>
  <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
  <% end %>
  <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
  <script src='//cdn.rawgit.com/googlemaps/js-rich-marker/gh-pages/src/richmarker-compiled.js' type='text/javascript'></script>
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<% end %>

<%= content_tag :div, id: "map-fullscreen", data: { hash: @hash } do %>
  <input id="map-input" class="map-controls" type="text" placeholder="Search Box"></input>
  <div id="map-canvas" style='width: 100%; height: 100%'></div>
<% end %>

